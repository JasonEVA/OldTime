#import <Foundation/Foundation.h>
#import <MJExtension/MJExtension.h>

@interface AppTaskModel : NSObject

@property (nonatomic, strong)  NSString  *title; // String 任务标题
@property (nonatomic)  long long  end; // Long 截至时间
@property (nonatomic, strong)  NSString  *projectName; // String 所属项目名称
@property (nonatomic, strong)  NSString  *content; // String 内容
@property (nonatomic, strong)  NSString  *priority; // String 优先级
@property (nonatomic)  NSInteger  level; // Int 任务层级,从1开始,1=根任务,2=子任务
@property (nonatomic, strong)  NSString  *id; // String 任务的id
@property (nonatomic)  NSInteger  allTask; // Int 所有任务数
@property (nonatomic)  NSInteger  finishTask; // Int 完成任务数
@property (nonatomic, strong) NSString *stateName; //状态
@property (nonatomic, strong) NSString *reason;
@property (nonatomic, strong) NSString *stateType;

@end
